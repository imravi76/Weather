<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IconTextView">
        <attr name="src" format="reference" />
        <attr name="text" format="string" />
        <attr name="textAppearance" format="reference" />
        <attr name="iconSize" format="dimension" />
        <attr name="iconTintColor" format="reference" />
        <attr name="iconTextVerticalMargin" format="dimension" />
        <attr name="iconTextHorizontalMargin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="WeatherGridItemView">
        <attr name="unit" format="string" />
        <attr name="value" format="string" />
        <attr name="unitTextAppearance" format="reference" />
        <attr name="valueTextAppearance" format="reference" />
        <attr name="unitValueHorizontalMargin" format="dimension" />
    </declare-styleable>
</resources>